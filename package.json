    "build": "rollup -c"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "jasmine-core": "^3.1.0",
    "karma": "^3.1.1",
    "karma-jasmine": "^3.3.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "phantomjs-prebuilt": "^2.1.16",
    "rollup": "^0.67.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-strip": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "serve": "^10.0.2"
