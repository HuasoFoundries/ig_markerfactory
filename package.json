{
  "name": "ig_markerfactory",
  "main": "markerfactory.js",
  "jsnext:main": "markerfactory.esm.js",
  "description": "Uses canvas to generate google.maps.Marker Icons from graphic fonts (i.e. font-awesome) on the fly",
  "version": "2.9.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HuasoFoundries/ig_markerfactory.git"
  },
  "bugs": {
    "url": "https://github.com/HuasoFoundries/ig_markerfactory/issues"
  },
  "homepage": "https://github.com/HuasoFoundries/ig_markerfactory#readme",
  "engines": {
    "node": "4.2.x"
  },
  "jspm": {
    "format": "esm",
    "main": "markerfactory.esm.js",
    "directories": {
      "lib": "dist"
    },
    "dependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.25"
    },
    "devDependencies": {
      "jquery": "npm:jquery@^3.0.0",
      "lodash": "npm:lodash@^4.13.1"
    },
    "overrides": {
      "npm:lodash@4.13.1": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      }
    }
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "eslint": "^3.0.1",
    "jspm": "^0.17.0-beta.13",
    "lodash": "^4",
    "mocha": "^2.4",
    "serve": "^1.4.0"
  }
}